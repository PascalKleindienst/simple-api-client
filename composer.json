{
  "name": "atog/simple-api-client",
  "description": "Simple API Client with cUrl",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Pascal Kleindienst",
      "email": "pascal.kleindienst@posteo.de"
    }
  ],
  "require": {
    "php": ">=5.5",
    "jyggen/curl": "4.0.x-dev",
    "illuminate/support": "5.1.*|5.2.*",
    "symfony/console": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "squizlabs/php_codesniffer": "~2.3"
  },
  "autoload": {
    "psr-4": {
      "Atog\\Api\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Atog\\Api\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  }
}
